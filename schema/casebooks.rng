          <assert test="replace(@sameAs,'#','')=(ancestor::tei:TEI/tei:text//tei:rs[@type='address']|ancestor::tei:TEI/tei:teiHeader//tei:residence)/@xml:id">ERROR: <name/>@sameAs must point to rs@type=address in body text</assert>
