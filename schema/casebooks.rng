  <define name="age">
    <element name="age">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(age) records a participant’s age as stated in the entry. [13.3.2.1. ]</a:documentation>
      <ref name="macro.phraseSeq.limited"/>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-age-age-check-recorded-constraint">
        <rule context="tei:age">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="not(@value|@atLeast|@atMost)">ERROR: <name/> needs to have at least one of the following: @value, @atLeast, @atMost</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-age-age-val-not-range-constraint">
        <rule context="tei:age">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="@value and (@atLeast|@atMost)">ERROR: <name/>@value cannot occur with either @atLeast or @atMost</report>
        </rule>
      </pattern>
      <ref name="att.global.attributes"/>
      <ref name="att.editLike.attribute.evidence"/>
      <ref name="att.dimensions.attribute.quantity"/>
      <ref name="att.dimensions.attribute.extent"/>
      <ref name="att.dimensions.attribute.precision"/>
      <ref name="att.dimensions.attribute.scope"/>
      <ref name="att.ranging.attribute.atLeast"/>
      <ref name="att.ranging.attribute.atMost"/>
      <ref name="att.responsibility.attribute.cert"/>
      <ref name="att.responsibility.attribute.resp"/>
      <ref name="att.source.attribute.source"/>
      <ref name="att.datable.attributes"/>
      <optional>
        <attribute name="unit">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives the unit in which the age is being measured. Permitted values: "seconds", "minutes", "hours", "days", "weeks", "fortnights", "months", "years", "decades", "centuries".</a:documentation>
          <choice>
            <value>seconds</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>minutes</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>days</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>weeks</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>fortnights</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>months</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>years</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>decades</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>centuries</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="value">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">may be any numerical value, expressed as an integer in Arabic numerals.</a:documentation>
          <ref name="data.count"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
