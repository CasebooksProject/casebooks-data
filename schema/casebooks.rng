  <define name="tei_age">
    <element name="age">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">records a participant’s age as stated in the entry. [13.3.2.1. Personal Characteristics]</a:documentation>
      <ref name="tei_macro.phraseSeq.limited"/>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-age-age-check-recorded-constraint-report-110">
        <rule context="tei:age">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="not(@value|@atLeast|@atMost)">ERROR: <name/> needs to have at least one of the following: @value, @atLeast, @atMost</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-age-age-val-not-range-constraint-report-111">
        <rule context="tei:age">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="@value and (@atLeast|@atMost)">ERROR: <name/>@value cannot occur with either @atLeast or @atMost</report>
        </rule>
      </pattern>
      <ref name="tei_att.global.attributes"/>
      <ref name="tei_att.editLike.attributes"/>
      <ref name="tei_att.datable.attributes"/>
      <ref name="tei_att.dimensions.attribute.quantity"/>
      <ref name="tei_att.dimensions.attribute.extent"/>
      <ref name="tei_att.dimensions.attribute.precision"/>
      <ref name="tei_att.dimensions.attribute.scope"/>
      <ref name="tei_att.ranging.attribute.atLeast"/>
      <ref name="tei_att.ranging.attribute.atMost"/>
      <optional>
        <attribute name="type">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.
Sample values include: 1] western; 2] sui; 3] subjective; 4] objective; 5] inWorld (in world) ; 6] chronological; 7] biological; 8] psychological; 9] functional</a:documentation>
          <data type="token">
            <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute name="unit">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives the unit in which the age is being measured. Permitted values: "days", "weeks", "months", "years".</a:documentation>
          <choice>
            <value>days</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>weeks</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>months</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>years</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="value">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">may be any numerical value, expressed as an integer in Arabic numerals.</a:documentation>
          <data type="nonNegativeInteger"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
