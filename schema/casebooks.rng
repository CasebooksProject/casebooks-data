        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:roleName[@type]">
          <report test="not(contains(document-uri(/),'master-lists'))">ERROR: @type is currently only used in the master name files</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-roleName-is_empty-constraint-report-107">
        <rule context="tei:roleName">
