        <rule context="tei:addName">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="descendant::surname|descendant::forename|descendant::roleName|descendant::nameLink|descendant::genName|descendant::name|descendant::addName">ERROR: <name/> contains a <value-of select="(descendant::surname|descendant::forename|descendant::roleName|descendant::nameLink|descendant::genName|descendant::name|descendant::addName)/local-name()"/>.</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-addName-is_empty-constraint-report-105">
        <rule context="tei:addName">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="not(normalize-space(.)) and not(descendant::tei:gap)">ERROR: <name/> is empty.</report>
        </rule>
      </pattern>
      <ref name="tei_att.casebooks.minimal.certainty.attributes"/>
      <ref name="tei_att.global.attributes"/>
      <ref name="tei_att.personal.attributes"/>
      <ref name="tei_att.typed.attributes"/>
