            <data type="gYearMonth"/>
            <data type="gMonthDay"/>
            <data type="time"/>
            <data type="dateTime"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="notBefore">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">defines the earliest possible time or beginning of a time range where this is not known precisely.</a:documentation>
