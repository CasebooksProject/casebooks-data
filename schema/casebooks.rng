              <value>binding</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>bleedthrough</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>blot</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>blotDel</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>code</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>copy</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>damage</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>del</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>faded</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>foxed</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>omitted</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>over</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
              <value>hand</value>
              <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
