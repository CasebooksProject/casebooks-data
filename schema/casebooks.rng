  <define name="att.casebooks.linking.attributes">
    <ref name="att.casebooks.linking.attribute.exclude"/>
    <ref name="att.casebooks.linking.attribute.xmlid"/>
  </define>
  <define name="att.casebooks.linking.attribute.exclude">
    <optional>
      <attribute name="exclude">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to elements that are in exclusive alternation with the current element.</a:documentation>
        <list>
          <oneOrMore>
            <ref name="data.pointer"/>
          </oneOrMore>
        </list>
      </attribute>
    </optional>
  </define>
  <define name="att.casebooks.linking.attribute.xmlid">
    <optional>
      <attribute name="xml:id">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) </a:documentation>
        <data type="ID"/>
      </attribute>
    </optional>
  </define>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-participant-exclude-empty-constraint">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@exclude]">
      <report test="normalize-space(@exclude) = ''">ERROR: <name/>@exclude is empty</report>
    </rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-participant-exclude-points-to-self-constraint">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@exclude]">
      <report test="tokenize(@exclude,'\s+')=concat('#',@xml:id)">ERROR: <name/>@exclude value (<value-of select="concat('#',@xml:id)"/>) points to same element as itself</report>
    </rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-exclude-not-xmlid-constraint">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@exclude]">
      <report test="not(@xml:id)">ERROR: <name/> with @exclude needs @xml:id</report>
    </rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-exclude-link-constraint">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@exclude and normalize-space(@exclude) != '']">
      <let name="elem" value="."/>
      <let name="other_elems" value="ancestor::tei:settingDesc//cb:*[local-name()=local-name($elem)][not(@xml:id=$elem/@xml:id)]"/>
      <assert test="every $exclude in $elem/tokenize(@exclude,'\s+') satisfies $exclude = $other_elems/concat('#',@xml:id)">ERROR: <name/>@exclude value(s) (<value-of select="$elem/tokenize(@exclude,'\s+')[not(.=$other_elems/concat('#',@xml:id))]"/>) does not point to a valid <name/>/@xml:id</assert>
    </rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-xmlid-not-target-constraint">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@xml:id]">
      <let name="elem" value="."/>
      <let name="other_elems" value="ancestor::tei:settingDesc//cb:*[local-name()=local-name($elem)][not(@xml:id=$elem/@xml:id)]"/>
      <assert test="concat('#',$elem/@xml:id) = $other_elems/tokenize(@exclude,'\s+')">ERROR: <name/>@xml:id must be the target of another <name/>@exclude</assert>
    </rule>
  </pattern>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-elem-xmlid-not-in-all-other-elem-constraint">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@xml:id]">
      <let name="elem" value="."/>
      <let name="all_elems" value="ancestor::cb:consultation//cb:*[local-name()=local-name($elem)][@exclude]"/>
      <assert test="normalize-space(@xml:id)!='' and count($all_elems[not(@xml:id=$elem/@xml:id)][tokenize(@exclude,'\s+') = $elem/concat('#',@xml:id)]) = count($all_elems[not(@xml:id=$elem/@xml:id)])">ERROR: <name/> is not the target of all the other <value-of select="$elem/local-name()"/>/@exclude</assert>
    </rule>
  </pattern>
  <define name="att.casebooks.minimal.responsibility.attributes">
    <ref name="att.casebooks.minimal.responsibility.attribute.cert"/>
    <ref name="att.casebooks.minimal.responsibility.attribute.resp"/>
  </define>
  <define name="att.casebooks.minimal.responsibility.attribute.cert">
