      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-list-gloss-list-must-have-labels-constraint-rule-65">
        <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:list[@type='gloss']">
