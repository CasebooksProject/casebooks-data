      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-metadata-time-no-children-constraint-rule-46">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time">
          <report test="(ancestor::tei:settingDesc|ancestor::tei:death|ancestor::tei:birth) and (*|text()[normalize-space()])">ERROR: Time should not have any children or text within it</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-exclude-same-container-constraint-rule-47">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time">
          <let name="current_time" value="."/>
          <let name="context" value="//ancestor::*[local-name()=('settingDesc','birth','death','event')][1]/local-name()"/>
          <let name="all_times" value="$current_time//ancestor::*[local-name()=$context]//tei:time"/>
          <report test="normalize-space(@exclude) != '' and count(tokenize(@exclude,'\s+')[.=$all_times[not(@xml:id=$current_time/@xml:id)]/concat('#',@xml:id)]) != count(tokenize(@exclude,'\s+'))">ERROR: <name/>@exclude value(s) (<value-of select="tokenize(@exclude,'\s+')[not(.=$all_times[not(@xml:id=$current_time/@xml:id)]/concat('#',@xml:id))]"/>) does not point to <value-of select="$context"/>//time/@xml:id</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-exclude-points-to-self-constraint-rule-48">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time[@exclude]">
          <report test="normalize-space(@exclude) != '' and tokenize(@exclude,'\s+')=concat('#',@xml:id)">ERROR: <name/>@exclude value (<value-of select="concat('#',@xml:id)"/>) points to same element as itself</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-exclude-not-xmlid-constraint-rule-49">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time[@exclude]">
          <report test="normalize-space(@exclude) != '' and not(@xml:id)">ERROR: <name/> needs @xml:id</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-xmlid-not-exclude-constraint-rule-50">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time[@xml:id]">
          <report test="not(@exclude)">ERROR: <name/> needs @exclude</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-xmlid-not-linked-to-same-container-constraint-rule-51">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time">
          <let name="current_time" value="."/>
          <let name="context" value="//ancestor::*[local-name()=('settingDesc','birth','death','event')][1]/local-name()"/>
          <let name="all_times" value="$current_time//ancestor::*[local-name()=$context]//tei:time"/>
          <report test="@exclude and not(concat('#',@xml:id) = $all_times/tokenize(@exclude,'\s+'))">ERROR: <name/> is not the target of any @exclude</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-exclude-missed-xmlid-constraint-rule-52">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time">
          <let name="current_time" value="."/>
          <let name="context" value="//ancestor::*[local-name()=('settingDesc','birth','death','event')][1]/local-name()"/>
          <let name="all_times" value="$current_time//ancestor::*[local-name()=$context]//tei:time"/>
          <report test="@exclude and normalize-space(@exclude)!='' and count( tokenize(@exclude,'\s+')[.= $all_times/concat('#',@xml:id)]) != count($all_times[@xml:id])-1">ERROR: <name/> does not link to (<value-of select="$all_times[not(@xml:id=$current_time/@xml:id)]/concat('#',@xml:id)[not(.=tokenize($current_time/@exclude,'\s+'))]"/>)</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-xmlid-not-in-all-other-times-constraint-rule-53">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time">
          <let name="current_time" value="."/>
          <let name="context" value="//ancestor::*[local-name()=('settingDesc','birth','death','event')][1]/local-name()"/>
          <let name="all_times" value="$current_time//ancestor::*[local-name()=$context]//tei:time"/>
          <report test="normalize-space(@xml:id)!='' and count($all_times[tokenize(@exclude,'\s+') = $current_time/concat('#',@xml:id)]) != count($all_times[not(@xml:id=$current_time/@xml:id)])">ERROR: <name/> is not the target of all the other <value-of select="$context"/>//time@exclude</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-notAfter-gt-notBefore-constraint-rule-54">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time[@notBefore and @notAfter]">
          <assert test="xs:time(@notAfter) &gt; xs:time(@notBefore)">ERROR: <name/>@notBefore must be less than @notAfter</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-to-gt-from-constraint-rule-55">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time[@to and @from]">
          <assert test="xs:time(@to) &gt; xs:time(@from)">ERROR: <name/>@from must be less than @to</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-has-illegal-mix-datable-attrs-constraint-rule-56">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time[@when|@when-custom]">
          <report test="count((@notBefore,@notBefore-custom,@notAfter,@notAfter-custom)) &gt; 0">ERROR: <name/> has @when and <value-of select="string-join(for $x in (@notBefore,@notBefore-custom,@notAfter,@notAfter-custom) return concat('@',$x/local-name()),', ')"/>
               </report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-time-time-has-space-in-attrs-constraint-rule-57">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:time">
          <let name="time_regex" value="'^((0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]:00|23:59:59)$'"/>
          <report test="(@when,@when-custom,@notBefore,@notBefore-custom,@notAfter,@notAfter-custom,@from,@to)[normalize-space(.)][not(matches(.,$time_regex))]">ERROR: Invalid <value-of select="(ancestor::tei:birth|ancestor::tei:death|ancestor::tei:settingDesc)/local-name()"/> time/<value-of select="for $x in (@when,@when-custom,@notBefore,@notBefore-custom,@notAfter,@notAfter-custom,@from,@to)[normalize-space(.)][not(matches(.,$time_regex))]/local-name() return concat('@',$x)"/> value '<value-of select="for $x in (@when,@when-custom,@notBefore,@notBefore-custom,@notAfter,@notAfter-custom,@from,@to)[normalize-space(.)][not(matches(.,$time_regex))] return $x"/>'.</report>
        </rule>
      </pattern>
      <ref name="tei_att.global.attribute.xmlid"/>
