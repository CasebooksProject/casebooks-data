  <define name="tei_foreign">
    <element name="foreign">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">marks up text written in a different language from the main language of the document, or from the text immediately surrounding it, using the xml:lang values en, la, el, fr, he (English, Latin, Greek, French, Hebrew). [3.3.2.1. Foreign Words or Expressions]</a:documentation>
      <ref name="tei_macro.phraseSeq"/>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-foreign-check-decls-if-exists-for-foreign-constraint-rule-10">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:foreign">
          <let name="text_root" value="/tei:TEI/tei:text"/>
