        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-passive-active-constraint-report-128">
        <rule context="tei:relation">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="(@passive and not(@active)) or (@active and not(@passive))">ERROR: <name/>@active and @passive must occur together</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-passive-active-intersection-constraint-report-129">
        <rule context="tei:relation">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="tokenize(@passive,'\s+') = tokenize(@active,'\s+')">ERROR: <name/> contains a one or more repeated tokens @active and @passive</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-link-passive-constraint-assert-71">
        <rule context="tei:relation">
          <assert xmlns:rng="http://relaxng.org/ns/structure/1.0" test="count(for $x in tokenize(@passive,'\s+') return //tei:listPerson//(tei:person|tei:org|tei:personGrp)[concat('#',@xml:id)=$x] ) = count(tokenize(@passive,'\s+'))">ERROR: <name/>@passive value(s) (<value-of select="string-join(for $x in tokenize(@passive,'\s+') return if (not($x = ancestor::tei:TEI//tei:listPerson//(tei:person|tei:org|tei:personGrp)/concat('#',@xml:id))) then $x else (),' ')"/>) does not point to listPerson//person, org or personGrp</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-link-active-constraint-assert-72">
        <rule context="tei:relation">
          <assert xmlns:rng="http://relaxng.org/ns/structure/1.0" test="count(for $x in tokenize(@active,'\s+') return //tei:listPerson//(tei:person|tei:org|tei:personGrp)[concat('#',@xml:id)=$x] ) = count(tokenize(@active,'\s+'))">ERROR: <name/>@active value(s) (<value-of select="string-join(for $x in tokenize(@active,'\s+') return if (not($x = ancestor::tei:TEI//tei:listPerson//(tei:person|tei:org|tei:personGrp)/concat('#',@xml:id))) then $x else (),' ')"/>) does not point to listPerson//person, org or personGrp</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-link-mutual-constraint-assert-73">
        <rule context="tei:relation">
          <assert xmlns:rng="http://relaxng.org/ns/structure/1.0" test="count(for $x in tokenize(@mutual,'\s+') return //tei:listPerson//(tei:person|tei:org|tei:personGrp)[concat('#',@xml:id)=$x] ) = count(tokenize(@mutual,'\s+'))">ERROR: <name/>@mutual value(s) (<value-of select="string-join(for $x in tokenize(@mutual,'\s+') return if (not($x = ancestor::tei:TEI//tei:listPerson//(tei:person|tei:org|tei:personGrp)/concat('#',@xml:id))) then $x else (),' ')"/>) does not point to listPerson//person, org or personGrp</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-active-sex-constraint-rule-114">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:relation">
          <let name="relation_elm" value="."/>
          <let name="active" value="$relation_elm/tokenize(@active,'\s+')"/>
          <let name="relations" value="doc(replace(replace(base-uri(.),'file:',''),'/casebooks-data/.+$','/casebooks-data/master-lists/relations.xml'))//tei:entry"/>
          <let name="bodies" value="//tei:listPerson//(tei:person|tei:personGrp)"/>
          <assert test="every $x in $bodies[concat('#',@xml:id)=$active] satisfies $x/lower-case(tei:sex[1])= $relations//tei:form[@n=$relation_elm/@name]//replace(tei:gen,'^n$','u')">ERROR: The sex of <name/>@active (<value-of select="for $x in $bodies[concat('#',@xml:id)=$active] return if ($x/lower-case(tei:sex[1]) != $relations//tei:form[@n=$relation_elm/@name]//tei:gen) then $x/concat('#',@xml:id) else ()"/>) is wrong for relation@name</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-mutual-sex-constraint-rule-115">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:relation">
          <let name="relation_elm" value="."/>
          <let name="mutual" value="$relation_elm/tokenize(@mutual,'\s+')"/>
          <let name="relations" value="doc(replace(replace(base-uri(.),'file:',''),'/casebooks-data/.+$','/casebooks-data/master-lists/relations.xml'))//tei:entry"/>
          <let name="bodies" value="//tei:listPerson//(tei:person|tei:personGrp)"/>
          <assert test="every $x in $bodies[concat('#',@xml:id)=$mutual] satisfies $x/lower-case(tei:sex[1])= $relations//tei:form[@n=$relation_elm/@name]//replace(tei:gen,'^n$','u')">ERROR: The sex of <name/>@mutual (<value-of select="for $x in $bodies[concat('#',@xml:id)=$mutual] return if ($x/lower-case(tei:sex[1]) != $relations//tei:form[@n=$relation_elm/@name]//tei:gen) then $x/concat('#',@xml:id) else ()"/>) is wrong for relation@name</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-reforkeyorname-constraint-assert-76">
