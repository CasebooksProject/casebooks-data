  <define name="tei_ref">
    <element name="ref">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(reference) supplies a link to another online location. [3.6. Simple Links and Cross-References 16.1. Links]</a:documentation>
      <ref name="tei_macro.paraContent"/>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-writing-link-to-us-with-domain-constraint-rule-58">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:ref">
          <let name="target" value="replace(@target,'^(https*://)*(dev|www).magicandmedicine.hps.cam.ac.uk','')"/>
          <report test="matches(@target,'(dev|www).magicandmedicine.hps.cam.ac.uk')">ERROR: Links to our site should be entered using their absolute path (ie. <value-of select="if (matches($target,'^/')) then $target else '/using-our-edition/guide-to-reading-the-cases'"/>)</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-writing-link-to-xmlid-constraint-rule-59">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:ref[matches(@target,'^#')]">
          <assert test="replace(@target,'^#','') = //@xml:id">ERROR: ref/@target points to nonexistant xml:id value on the page (<value-of select="@target"/>)</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-ref-no-type-target-not-site-self-mailto-constraint-rule-60">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:ref[not(@type) or normalize-space(@type)='']">
          <assert test="matches(@target,'^(https*://|/|mailto:|#)')">ERROR: ref without @type must point to external site, us or an email address using mailto:</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-ref-no-type-pointing-to-case-constraint-rule-61">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:ref[matches(@target,'^(MS_Ashmole_)*\d\d\d-(f\d\d\d\d[A-E]*[rv]*|p\d\d\d\d[A-E]*)-\d(\.xml)*$','i') or matches(@target,'^CASE\d+$')]">
          <assert test="@type='case'">ERROR: ref pointing to another case must have type="case"</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-ref-no-case-filenames-in-live-constraint-rule-62">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:ref[matches(@target,'^(MS_Ashmole_)*\d\d\d-(f\d\d\d\d[A-E]*[rv]|p\d\d\d\d[A-E]*)-\d(\.xml)*$','i')]">
          <let name="location" value="replace(replace(base-uri(.),'file:',''),'^.*/casebooks-data/([^/]+)/.+$','$1')"/>
          <report test="$location='04-live'">ERROR: ref pointing to a case file rather than case number</report>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-ref-type-case-must-point-to-case-constraint-rule-63">
        <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:ref[@type='case']">
          <assert test="matches(@target,'^(MS_Ashmole_)*\d\d\d-(f\d\d\d\d[A-E]*[rv]*|p\d\d\d\d[A-E]*)-\d(\.xml)*$','i') or matches(@target,'^CASE\d+$')">ERROR: ref/@type="case" must point to a valid case number or case filename</assert>
        </rule>
      </pattern>
      
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-ref-refAtts-constraint-report-67">
        <rule context="tei:ref">
          <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="@target and @cRef">Only one of the
	attributes @target' and @cRef' may be supplied on <name/>
               </report>
        </rule>
      </pattern>
      <ref name="tei_att.global.attributes"/>
      <ref name="tei_att.internetMedia.attributes"/>
      <ref name="tei_att.declaring.attributes"/>
      <ref name="tei_att.cReferencing.attributes"/>
      <attribute name="target">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the destination of the reference by supplying one or more URI References</a:documentation>
        <list>
          <oneOrMore>
            <data type="anyURI"/>
          </oneOrMore>
        </list>
      </attribute>
      <optional>
        <attribute name="type">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">values are "case" (points to case file), "letter" (points to letter file), "note" (points to note file), "person" (points to identified entity - i.e. person, org, personGrp), "text" (points to text file) and "protected" (points to resource behind a paywall).</a:documentation>
          <choice>
            <value>case</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>letter</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>note</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>person</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>text</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <value>protected</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          </choice>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
