          </oneOrMore>
        </list>
      </attribute>
    </optional>
  </define>
  <define name="att.casebooks.participants.certainty.attributes">
    <ref name="att.casebooks.minimal.responsibility.attributes"/>
    <ref name="att.casebooks.participants.certainty.attribute.evidence"/>
  </define>
  <define name="att.casebooks.participants.certainty.attribute.evidence">
    <optional>
      <attribute name="evidence">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
        <choice>
          <value>internal</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">there is internal evidence to support the editorial decision.</a:documentation>
          <value>external</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">there is external evidence to support the editorial decision.</a:documentation>
          <value>inferred</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the interpretation has been inferred.</a:documentation>
          <value>conjecture</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the editor has based the interpretation on her or his expertise.</a:documentation>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="att.casebooks.participants.attributes">
    <ref name="att.casebooks.participants.certainty.attributes"/>
    <ref name="att.casebooks.participants.attribute.reason"/>
    <ref name="att.casebooks.participants.attribute.present"/>
    <ref name="att.casebooks.participants.attribute.ref"/>
  </define>
  <define name="att.casebooks.participants.attribute.reason">
    <optional>
      <attribute name="reason">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Placement</a:documentation>
        <choice>
          <value>deleted</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <value>notGiven</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <value>msDamage</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <value>notUnderstood</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="att.casebooks.participants.attribute.present">
    <optional>
      <attribute name="present">
        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">present</a:documentation>
        <choice>
          <value>yes</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <value>no</value>
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
