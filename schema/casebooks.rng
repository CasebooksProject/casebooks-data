                <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
              </data>
            </oneOrMore>
          </list>
        </attribute>
      </optional>
      <optional>
        <attribute name="sex">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the sex of the persona.</a:documentation>
