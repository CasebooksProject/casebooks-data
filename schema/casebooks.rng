  <define name="tei_person">
    <element name="person">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains biographical data about a person, such as name, age, sex, residence, occupation. [13.3.2. The Person Element 15.2.2. The Participant Description]</a:documentation>
      <choice>
        <oneOrMore>
          <ref name="tei_model.pLike"/>
        </oneOrMore>
        <zeroOrMore>
          <choice>
            <ref name="tei_model.personPart"/>
            <ref name="tei_model.global"/>
