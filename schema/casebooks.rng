  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-att.casebooks.linking-cb-participant-exclude-empty-constraint-rule-131">
    <rule xmlns:rng="http://relaxng.org/ns/structure/1.0" context="cb:*[local-name()=('querent','subject','object')][@exclude]">
