      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-needs-objects-constraint-assert-70">
        <rule context="tei:relation">
          <assert xmlns:rng="http://relaxng.org/ns/structure/1.0" test="count((@passive,@active,@mutual)) gt 0">ERROR: <name/> needs @active/@passive or @mutual</assert>
        </rule>
      </pattern>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="casebooks-relation-relation-mutual-count-constraint-report-127">
